{% extends "base.html.twig" %}
{% block body %}
    <div class = "container">
        {{ form_start(form) }}
        {{ form_row(form.firstname) }}
        {{ form_row(form.lastname) }}
        {{ form_row(form.phone) }}
        {{ form_row(form.email) }}
        <br/>
        {{ form_row(form.plainPassword.first) }}
        {{ form_row(form.plainPassword.second) }}
        <br/>
        <button type="submit">Save !</button>
        {{ form_end(form) }}
    </div>
{% endblock %}